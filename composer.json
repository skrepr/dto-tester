{
    "name": "skrepr/dto-tester",
    "description": "Skrepr Data Transfer Object Tester",
    "keywords": ["unittest", "phpunit", "dto", "entity", "data transfer object", "tester"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1"
    },
    "authors": [
        {
            "name": "Michael Erkens",
            "email": "michael@skrepr.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Skrepr\\DtoTester\\": "src/"
        }
    }
}
